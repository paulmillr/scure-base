{
  "extends": "@paulmillr/jsbt/tsconfig.esm.json",
  "compilerOptions": {
    "module": "es2020",
    "moduleResolution": "bundler",
    "noUncheckedIndexedAccess": true,
    "outDir": "lib/esm",
    "isolatedDeclarations": true
  },
  "include": ["index.ts", "src"],
  "exclude": ["node_modules", "lib"]
}
